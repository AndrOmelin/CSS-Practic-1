{{!-- <div class="page"> --}}
  {{!-- <div class="page-content"> --}}
      {{!-- <header class="page_block"> --}}
       <header class="header">
            <div class="col-3">
                 {{> logo}}
            </div>{{!-- <div class="col-3"> --}}

              <nav class="header_nav">
                   <div class="header_nav_right">
                        {{> nav list=header}}
                   </div>
              </nav>{{!-- <nav class="header_nav"> --}}
       </header>

     {{#with main}}
       <main class="main">
          <img src="{{image}}" alt="main image" class="main_image">

            <div class="main_content">
                 <h1 class="main_heading">
                     {{heading}} 
                 </h1>
                 <p class="main_description">
                    {{description}}
                 </p>
                 <div>
                 {{!-- <div class="main_button_div"> --}}
                      <a href="{{href}}" class="button main_button">{{button}}</a>
                 </div>
            </div>{{!-- <div class="hero_content"> --}}
               {{!-- <img src="{{href}}" alt="main image" class="main_image"> --}}
       </main>
     {{/with}}  

       <div class="featured">
             {{#each featured}}
                     {{> cardFeatured data=this}}
             {{/each}}
       </div>{{!-- <div class="featured"> --}}

     {{#with form}}
       <div class="form">
            <div class="form_content">
                 <h3 class="form_title">{{title}}</h3>
                  <p class="form_info">{{info}}</p>
               {{!-- <div class="form_input">
                    <a href="{{href}}" class="form_button">{{button}}</a>
               </div>     --}}
            </div>
               <div class="form_input">
                    <a href="{{href}}" class="button form_button">{{button}}</a>
               </div> 
       </div>
     {{/with}}

       {{!-- <footer class="page_block"> --}}
         {{!-- {{> divider}} --}}
         <footer class="row footer">
          {{!-- {{> divider}} --}}
          {{!-- <br> --}}
           <div class="col-3">
                {{> logo}}
                <span class="footer-copyright">Copyright 2023-2024</span>
           </div> {{!-- <div class="col-3"> --}} 
            {{#each footer}}
              <div class="col-3">
                   <span class="footer_title">{{title}}</span>
                   {{> nav list=links isVertical=true}}
              </div>
            {{/each}}
         </footer>
       {{!-- </footer> --}}
   
  {{!-- </div class="page-content"> --}}
{{!-- </div class="page"> --}}          